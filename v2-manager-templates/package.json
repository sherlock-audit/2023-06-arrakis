{
  "name": "@arrakisfi/v2-manager-templates",
  "version": "0.1.0",
  "description": "Arrakis V2 Manager Templates",
  "license": "MIT",
  "main": "dist/src/addresses.js",
  "types": "dist/src/addresses.d.ts",
  "files": [
    "contracts",
    "!contracts/__mocks__",
    "src"
  ],
  "scripts": {
    "build": "yarn compile && yarn compile-hardhat && npx tsc",
    "compile": "forge build --force",
    "compile-hardhat": "npx hardhat compile --force",
    "clean": "rm -rf dist && npx hardhat clean",
    "deploy": "npx hardhat deploy ",
    "format": "prettier --write .",
    "format:check": "prettier --check '*/**/*.{js,sol,json,md,ts}'",
    "lint": "eslint --cache . && yarn lint:sol",
    "lint:ts": "eslint -c .eslintrc.json --ext \"**/*.ts\" \"**/*.test.ts\"",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test": "forge test -vvv",
    "coverage": "forge coverage",
    "test-hardhat": "npx hardhat test",
    "verify": "npx hardhat etherscan-verify"
  },
  "devDependencies": {
    "@arrakisfi/v2-core": "1.1.9",
    "@arrakisfi/v3-lib-0.8": "0.1.4",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers",
    "@nomiclabs/hardhat-etherscan": "3.1.4",
    "@nomiclabs/hardhat-waffle": "2.0.3",
    "@openzeppelin/contracts": "4.8.0",
    "@openzeppelin/contracts-upgradeable": "4.8.0",
    "@tsconfig/recommended": "1.0.1",
    "@typechain/ethers-v5": "10.2.0",
    "@typechain/hardhat": "6.1.5",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "20.1.3",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@uniswap/v3-core": "1.0.1",
    "@uniswap/v3-periphery": "1.4.3",
    "@uniswap/v3-sdk": "3.9.0",
    "chai": "4.3.7",
    "dotenv": "16.0.3",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "ethereum-waffle": "4.0.10",
    "ethers": "5.7.2",
    "hardhat": "2.12.5",
    "hardhat-deploy": "0.11.22",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "8.0.2",
    "jsbi": "^4.3.0",
    "lint-staged": "13.2.2",
    "node-fetch": "3.3.0",
    "pinst": "^3.0.0",
    "prettier": "2.8.1",
    "prettier-plugin-solidity": "1.1.1",
    "solhint": "3.4.1",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "0.8.2",
    "ts-generator": "0.1.1",
    "ts-node": "10.9.1",
    "typechain": "8.1.1",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint -c .eslintrc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ArrakisFinance/v2-manager-templates"
  },
  "keywords": [
    "arrakis",
    "uniswap",
    "v3",
    "market",
    "making",
    "AMM",
    "solidity",
    "0.8"
  ],
  "author": "Arrakis",
  "bugs": {
    "url": "https://github.com/ArrakisFinance/v2-manager-templates/issues"
  },
  "homepage": "https://github.com/ArrakisFinance/v2-manager-templates#readme"
}
